{
  "name": "wander-cache",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "postinstall": "patch-package",
    "update-prices": "dotenv --ignore-missing -- bash -c 'curl -X GET ${COOLIFY_URL:-${APP_URL:-http://localhost:3000}}/api/cron/update-prices -H \"Authorization: Bearer $CRON_SECRET\"'",
    "process-transak-savings": "dotenv --ignore-missing -- bash -c 'curl -X GET ${COOLIFY_URL:-${APP_URL:-http://localhost:3000}}/api/cron/process-transak-savings -H \"Authorization: Bearer $CRON_SECRET\"'",
    "update-flp-tokens": "dotenv --ignore-missing -- bash -c 'curl -X GET ${COOLIFY_URL:-${APP_URL:-http://localhost:3000}}/api/cron/update-flp-tokens -H \"Authorization: Bearer $CRON_SECRET\"'"
  },
  "dependencies": {
    "@permaweb/aoconnect": "^0.0.77",
